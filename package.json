{
  "name": "simple-express-api-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "test": "jest --coverage",
    "clean": "rimraf dist",
    "build": "babel src -d dist"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushilnayak/simple-express-api-starter.git"
  },
  "bugs": {
    "url": "https://github.com/sushilnayak/simple-express-api-starter/issues"
  },
  "homepage": "https://github.com/sushilnayak/simple-express-api-starter#readme",
  "description": "",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-validator": "^6.9.0",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^3.7.0",
    "swagger-ui-dist": "^3.38.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "rimraf": "^2.7.1",
    "superagent": "^5.3.1",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.{spec,test}.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ]
  }
}
